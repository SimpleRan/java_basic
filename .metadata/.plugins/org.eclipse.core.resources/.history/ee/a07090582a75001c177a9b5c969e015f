package accessmodifier;

public class Warrior { //정의부, 실행은 Main에서 할 예정임
   
	public int hp;
	public int stranght;
	public String name;
	public int lv;
	public int money;
	
	// 생성자를 만들어주시되, name만 사용자에게 입력받고
	// 나머지는 hp20, str 3, lv 1, money 0
	
	public Warrior(String n) {
		name = n;
		System.out.println(name+"님 ");
	hp = 20;
	stranght = 3 ;
	lv = 1;
	money = 0;
	
	
 }
}