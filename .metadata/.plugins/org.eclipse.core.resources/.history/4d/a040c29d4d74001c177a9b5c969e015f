package constructor;

public class Car {
    // 자동차의 상태(속성)를 나타내는 변수들
	public int gas; // 연료량
	public int speed ; // 속도
	public String owner; // 주인
	
	//생선자 정의
	// 생성자sms 리턴타입을 가지지 않고, 클래서명을 그대로 메서드명으로 씁니다.
	// new 키워드로 생성할 때 딱 한 번만 실행됩니다.
	public Car (String o) {
		gas = 100; // 출고시 만땅
		speed = 0; // 출고시 0
		owner = o;
	}
}
